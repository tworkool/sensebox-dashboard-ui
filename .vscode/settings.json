{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "typescriptreact"
  ],
  "terminal.integrated.cwd": "${workspaceFolder}/webapp",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  }
}